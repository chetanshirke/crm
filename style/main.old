#page {
	margin:auto;
	width:1920px;
	border:0px red solid;
	background:url(../images/bg.jpg); 
}
a:active, a:focus {
	outline:none;
}
#right_col a {
	color:#06F;
	text-decoration:underline;
}
.blue { color:#183883;}
.text_bold { font-weight:bold;}
